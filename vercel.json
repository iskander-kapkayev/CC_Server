{
    "version": 2,
    "builds": [{
        "src": "server.mjs",
        "use": "@vercel/node"
    }],
    "rewrites": [{ "source": "/(.*)", "destination": "server.mjs"}],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
